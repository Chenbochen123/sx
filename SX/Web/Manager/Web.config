<?xml version="1.0"?>
<configuration>
    <system.web>
        <!--Form认证设置Start-->
        <authorization>
            <!--<deny users="?"/>-->
        </authorization>
        <!--Form认证设置End-->
    </system.web>
    <!-- Begin 排除不需要验证的目录或文件-->
    <location path="Authentication/Login.aspx">
        <system.web>
            <authorization>
                <allow users="*"/>
            </authorization>
        </system.web>
    </location>
    <location path="Authentication/HostRegister.aspx">
        <system.web>
            <authorization>
                <allow users="*"/>
            </authorization>
        </system.web>
    </location>
    <!-- End 排除不需要验证的目录或文件-->
</configuration>
