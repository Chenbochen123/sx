            {
                cellText.Hyperlink = link;
                cellText.CellStyle = style;
                treeNodeDetailList.Add(CreateExtTreeNode(dic["nodeId"].ToString(), dic["nodeText"].ToString(), Convert.ToInt32(dic["nodeLevel"])));
