                                                    <Items>
                                                        <ext:ListItem Text="甲" Value="1">
                                                        </ext:ListItem>
                                                        <ext:ListItem Text="乙" Value="2">
                                                        </ext:ListItem>
                                                        <ext:ListItem Text="丙" Value="3">
                                                        </ext:ListItem>
                                                    </Items>
                                                    <Triggers>
                                                        <ext:FieldTrigger Icon="Clear" Qtip="清空">
                                                        </ext:FieldTrigger>
                                                    </Triggers>
                                                    <Listeners>
                                                        <TriggerClick Handler="this.setValue('');" />
                                                    </Listeners>
                                                </ext:ComboBox>
                                         <ext:ComboBox ID="cbxxinghao1" runat="server" Disabled="false" Width="230" AnchorHorizontal="100%" FieldLabel="隔离剂型号" />
                                              
